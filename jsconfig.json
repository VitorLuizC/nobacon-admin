{
  "typeAcquisition": {
    "enable": true
  },
  "compilerOptions": {
    "module": "es2015",
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@store/*": ["src/store/*"],
      "@styles/*": ["src/assets/styles/*"],
      "@images/*": ["src/assets/images/*"],
      "@lib/*": ["src/lib/*"],
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"]
    }
  }
}
